{
  "name": "SlidePlayer Skipper",
  "description": "Skip share buttons when downloading a presentation on SlidePlayer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
   "tabs"
  ],
  "icons": {
    "16": "/images/icon-16.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://slideplayer.com/*","https://slideplayer.com/*"],
      "js": ["script.js"]
    }
  ]
}